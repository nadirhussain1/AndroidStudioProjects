<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:topLeftRadius="4.0px" android:topRightRadius="4.0px" android:bottomLeftRadius="4.0px" android:bottomRightRadius="4.0px" />
            <gradient android:startColor="@color/black" android:endColor="@color/black" android:angle="-90.0" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:topLeftRadius="4.0px" android:topRightRadius="4.0px" android:bottomLeftRadius="4.0px" android:bottomRightRadius="4.0px" />
                <gradient android:startColor="@color/black" android:endColor="@color/black" android:angle="-90.0" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:topLeftRadius="4.0px" android:topRightRadius="4.0px" android:bottomLeftRadius="4.0px" android:bottomRightRadius="4.0px" />
                <gradient android:startColor="@color/white" android:endColor="@color/white" android:angle="90.0" />
            </shape>
        </clip>
    </item>
</layer-list>